<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="CopyFilesTool" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="GnuDebug">
				<Option output="bin\GnuDebug\copyfiles" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\GnuDebug\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add option="-DGNUCOMPATIBLEOS" />
				</Compiler>
			</Target>
			<Target title="GnuRelease">
				<Option output="bin\GnuRelease\copyfiles" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\GnuRelease\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-DGNUCOMPATIBLEOS" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
			<Target title="WinRelease">
				<Option output="..\..\physgameengine\data\copyfiles" prefix_auto="1" extension_auto="1" />
				<Option working_dir="..\..\physgameengine\" />
				<Option object_output="obj\WinRelease\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-Os" />
					<Add option="-DWINDOWSCOMPATIBLEOS" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-static-libgcc" />
					<Add option="-static-libstdc++" />
				</Linker>
			</Target>
			<Target title="WinDebug">
				<Option output="..\..\physgameengine\data\copyfiles" prefix_auto="1" extension_auto="1" />
				<Option working_dir="..\..\physgameengine\" />
				<Option object_output="obj\WinDebug\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option parameters="WinDebug windows" />
				<Compiler>
					<Add option="-g" />
					<Add option="-DWINDOWSCOMPATIBLEOS" />
				</Compiler>
				<Linker>
					<Add option="-static-libgcc" />
					<Add option="-static-libstdc++" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fexceptions" />
		</Compiler>
		<Linker>
			<Add option='&quot;-Wl,--enable-auto-import&quot;' />
		</Linker>
		<Unit filename="main.cpp" />
		<Extensions>
			<code_completion />
			<debugger />
			<envvars />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
