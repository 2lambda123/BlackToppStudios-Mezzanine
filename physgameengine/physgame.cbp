					<Add option="-g" />
					<Add option="`sdl-config --cflags`" />
					<Add option="-fexceptions" />
					<Add option="-fPIC" />
					<Add option="-DOGRE_GUI_GLX" />
					<Add option="-DOGRE_CONFIG_LITTLE_ENDIAN" />
					<Add option="-DLINUX" />
					<Add directory="libincludes/linux/ogresrc/OgreMain/include" />
					<Add directory="libincludes/linux/ogresrc/include" />
					<Add directory="libincludes/linux/bulletsrc/bullet-2.76/src" />
					<Add directory="libincludes/linux/bulletsrc/bullet-2.76/src/LinearMath" />
				</Compiler>
				<Linker>
					<Add option="`sdl-config --libs`" />
					<Add library="OgreMain" />
					<Add library="Bullet" />
					<Add directory="data/linux/bullet" />
					<Add directory="data/linux/ogre" />
				</Linker>
				<ExtraCommands>
					<Add before="data/linux/checklibs.sh" />
					<Add after="mkdir bin/LinuxDebug/data/linux -p" />
					<Add after="cp data/linux/plugins.cfg bin/LinuxDebug/data/linux/" />
					<Add after="cp data/linux/settings.cfg bin/LinuxDebug/data/linux/" />
					<Add after="cp data/linux/ogre/ bin/LinuxDebug/data/linux/ -r" />
