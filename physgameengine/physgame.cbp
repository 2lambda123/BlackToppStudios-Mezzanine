<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="physgame" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Option extended_obj_names="1" />
		<Build>
			<Target title="WinDebug">
				<Option output="bin/WinDebug/physgame.dll" prefix_auto="1" extension_auto="0" />
				<Option object_output="obj/WinDebug/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add directory="libincludes/windows/ogre" />
					<Add directory="libincludes/windows/sdl" />
					<Add directory="libincludes/windows/bullet" />
				</Compiler>
				<Linker>
					<Add option="-Wl,--enable-runtime-pseudo-reloc" />
					<Add option="-Wl,--enable-auto-image-base" />
					<Add option="-Wl,--add-stdcall-alias" />
					<Add library="SDL" />
					<Add library="OgreMain" />
					<Add library="data/windows/bullet/libbulletdynamics.lib" />
					<Add library="data/windows/bullet/libbulletmath.lib" />
					<Add library="data/windows/bullet/libbulletcollision.lib" />
					<Add directory="data/windows/sdl" />
					<Add directory="data/windows/ogre" />
					<Add directory="data/windows/bullet" />
				</Linker>
				<ExtraCommands>
					<Add after="cmd /c copy .\data\windows\ogre\OgreMain.dll .\bin\WinDebug" />
					<Add after="cmd /c copy .\data\windows\sdl\SDL.dll .\bin\WinDebug" />
					<Add after="cmd /c mkdir .\bin\WinDebug\data" />
					<Add after="cmd /c mkdir .\bin\WinDebug\data\windows" />
					<Add after="cmd /c copy .\data\windows\plugins.cfg .\bin\WinDebug\data\windows" />
					<Add after="cmd /c copy .\data\windows\settings.cfg .\bin\WinDebug\data\windows" />
					<Add after="cmd /c mkdir .\bin\WinDebug\data\windows\ogre" />
					<Add after="cmd /c copy .\data\windows\ogre\RenderSystem_GL.dll .\bin\WinDebug\data\windows\ogre" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Target title="LinuxDebug">
				<Option output="bin/LinuxDebug/libphysgame.so" prefix_auto="1" extension_auto="0" />
				<Option object_output="obj/LinuxDebug/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add option="`sdl-config --cflags`" />
					<Add option="`pkg-config --cflags OGRE`" />
					<Add option="-fexceptions" />
					<Add option="-DOGRE_GUI_GLX" />
					<Add option="-DOGRE_CONFIG_LITTLE_ENDIAN" />
					<Add directory="libincludes/linux/ogre" />
					<Add directory="libincludes/linux/bullet" />
				</Compiler>
				<Linker>
					<Add option="`sdl-config --libs`" />
					<Add option="`pkg-config --libs OGRE`" />
					<Add library="OgreMain" />
					<Add library="Bullet" />
					<Add directory="data/linux/bullet" />
				</Linker>
				<ExtraCommands>
					<Add after="mkdir bin/LinuxDebug/data/linux -p" />
					<Add after="cp data/linux/plugins.cfg bin/LinuxDebug/data/linux" />
					<Add after="cp data/linux/settings.cfg bin/LinuxDebug/data/linux" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
